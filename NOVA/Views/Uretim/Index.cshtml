        function KapatilacakIsEmriSec(btn, value)
        {
            event.stopPropagation();

            if (KapatilacakIsEmirleri.filter(item => item.ISEMRINO === value).length > 0)
            {
                KapatilacakIsEmirleri = KapatilacakIsEmirleri.filter(item => item.ISEMRINO !== value);

                btn.getElementsByTagName("i")[0].className = "bi bi-check-circle";
                btn.getElementsByTagName("i")[0].style = "color:red;font-size:x-large;";
            }
            else
            {
                KapatilacakIsEmirleri.push({ "ISEMRINO": value });

                btn.getElementsByTagName("i")[0].className = "bi bi-check-circle-fill";
                btn.getElementsByTagName("i")[0].style = "color:#71dd37;font-size:x-large;";
            }

            if(KapatilacakIsEmirleri.length > 0)
            {
                $('#isemirlerinikapatbtn').show();
            }
            else
            {
                $('#isemirlerinikapatbtn').hide();
            }
