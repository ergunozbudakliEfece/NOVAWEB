                        })

                        $("#guncelleButton").attr("data-seri", decodedText);

                        $("#grup1").hide();
                        $("#grup2").show();
                        lastResult1 = 0;
                    }




                }

                html5QrcodeScanner1 = new Html5QrcodeScanner(
