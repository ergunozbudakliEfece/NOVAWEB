            --z-fixed: 100;
            --container-radius: 0.25rem;
            --nav-width: 250px;
        }

        html {
            height: 100svh;
            box-sizing: border-box;
        }

        body {
            height: inherit;
            box-sizing: inherit;
            background-color: var(--body-color);
            color: var(--text-color);
            font-family: var(--body-font);
            padding-left: 50px;
            transition: padding-left 0.5s;
        }

        .overlay {
            z-index: 5000 !important;
            position: fixed;
            top: 0;
            right: 0;
            left: 0;
            bottom: 0;
            z-index: 40;
            display: flex;
            justify-content: center;
            align-items: center;
            display: flex;
            background: #0000004d;
            backdrop-filter: blur(3px);
            -webkit-backdrop-filter: blur(3px);
        }

            .overlay > img {
                height: 120px;
                width: 120px;
                animation: Beat 2s infinite ease-in-out;
            }

        @@keyframes Beat {
            0% {
                transform: scale(0.8)
            }

            50% {
                transform: scale(1.2)
            }

            100% {
                transform: scale(0.8)
            }
        }

        #exit {
            display: none;
        }
    </style>
    <script>
        function ShowProgress() {
            document.body.style.overflow = 'hidden';
            document.activeElement.blur();
            $('#progress').fadeIn('fast').css('display', 'flex');
        }
        function HideProgress() {
            document.body.style.overflow = 'auto';
            $('#progress').fadeOut('slow');
        }

        function ShowExit() {
            document.body.style.overflow = 'hidden';
            document.activeElement.blur();
            $('#exit').fadeIn('fast').css('display', 'flex');
        }
        function HideExit() {
            document.body.style.overflow = 'auto';
            $('#exit').fadeOut('slow');
        }
