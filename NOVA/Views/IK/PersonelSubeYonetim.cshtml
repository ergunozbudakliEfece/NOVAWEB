
@{
    Layout = "/Views/Shared/_Layout_personel.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
    <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
    <style>
        body {
            padding: 0 0 0 3rem !important;
        }
    </style>
    <title>Nova | Şube Bazında Personel Bilgi Yönetimi</title>
</head>
<body onclose="close()">
    <div class="container-xxl flex-grow-1 container-p-y">
        <div class="row">
            <div class="col">
                <select data-bs-toggle="tooltip" data-bs-offset="0,4" data-bs-placement="top" data-bs-html="true" data-bs-original-title="<i class='bx bxs-info-circle bx-xs' ></i> <span>Seçilen personele göre liste güncellenir</span>" onchange="location.href='@Url.Action("GetSube", "IK")/'+value" class="form-select dropdown-toggle secim" id="modules">
                    <option id="0" selected="selected" value="0">PERSONEL SEÇİMİ</option>

                    @{

                        foreach (var item in ViewBag.Personels)
                        {
                            if (ViewBag.Select == item.USER_ID)
                            {
                                <option value=@item.USER_ID selected>@item.ISIM @item.SOYISIM</option>
                            }
                            else
                            {
                                <option value=@item.USER_ID>@item.ISIM @item.SOYISIM</option>
                            }



                        }




                    }

                </select>
            </div>
        </div>
        <div class="row mt-3">
            <div class="col-md-12">
                <div class="card mb-4">
                    <div class="card-header">
                        <div class="row">
                            <div class="col">
                                <h5>Personel Bilgileri Yönetimi</h5>
                            </div>
                        </div>
                        <div class="row mt-3">
                            <div class="col">
                                @{string s = "10000.png";
                                    int p = 0;
                                    string yetki = "disabled";
                                    if (ViewBag.Yetki != null && ViewBag.Personel != null)
                                    {
                                        yetki = "";
                                    }
                                    if (ViewBag.Personel != null)
                                    {

                                        s = ViewBag.Personel.USER_ID + ".png?" + DateTime.Now.Ticks.ToString();
                                        p = ViewBag.Personel.USER_ID;
                                    } }
                                @using (Html.BeginForm("UploadFileSubeYonetim", "IK", new { id = @p }, FormMethod.Post, new { enctype = "multipart/form-data" }))
                                {
                                    <div class="d-flex align-items-start align-items-sm-center gap-4">


                                        <img src="~/assets/img/avatars/@s" alt="user-avatar" class="d-block rounded" height="100" width="100" id="blah" onerror="javascript:this.src='/assets/img/avatars/10000.png'">
                                        <div class="button-wrapper">
                                            <label for="upload" class="btn btn-primary me-2 mb-4" tabindex="0">
                                                <span class="d-none d-sm-block">Fotoğraf Yükle</span>
                                                <i class="bx bx-upload d-block d-sm-none"></i>
                                                <input type="file" id="upload" name="file" class="account-file-input" hidden="" accept="image/png, image/jpeg" onchange="readURL(this);" @yetki>

                                            </label>
                                            <button id="resimKaydet" type="submit" class="btn btn-primary me-2 mb-4" disabled>
                                                Kaydet

                                            </button>


                                            <p class="text-muted mb-0">Sadece JPG,GIF veya PNG formatında maksimum 800 kb boyutunda olmalıdır.</p>

                                        </div>

                                    </div>

                                }
                            </div>


                        </div>
                    </div>


                    <!-- Account -->
                    <hr class="my-0">

                    <div class="card-body" id="table">
                        @if (ViewBag.Personel == null && ViewBag.Ekle == null)
                        {
                            using (Html.BeginForm("PersonelSubeYonetim", "IK", FormMethod.Post, new { @id = "myPersonalForm" }))
                            {
                                if (Session["Alert"] != null)
                                {
                                    if (Session["Alert"].ToString() == "Personel başarılı bir şekilde eklenmiştir.")
                                    {
                                        <div class="alert alert-success" role="alert">
                                            <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
                                            @Session["Alert"].ToString()
                                        </div>
                                    }
                                    else if (Session["Alert"].ToString() == "Personel kaydı başarısız!")
                                    {
                                        <div class="alert alert-danger" role="alert">
                                            <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
                                            @Session["Alert"].ToString()
                                        </div>
                                    }
                                }
                                <div class="row">
                                    <div class="col">
                                        <label for="firstName" class="form-label">İsim</label>
                                        <input class="form-control" type="text" id="ISIM" name="ISIM" disabled>
                                    </div>
                                    <div class="col">
                                        <label for="firstName" class="form-label">Soyisim</label>
                                        <input class="form-control" type="text" id="SOYISIM" name="SOYISIM" disabled>
                                    </div>

                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="firstName" class="form-label">TC KİMLİK NUMARASI</label>
                                        <input class="form-control" type="text" id="TCKN" name="TCKN" oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');" maxlength="11" disabled>
                                    </div>
                                    <div class="col-6">
                                        <label for="CINSIYET" class="form-label">CİNSİYET</label>
                                        <select id="CINSIYET" name="CINSIYET" class="form-select duzenle" disabled>


                                            <option selected></option>

                                            <option value="KADIN">KADIN</option>


                                            <option value="ERKEK">ERKEK</option>





                                        </select>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="firstName" class="form-label">DOĞUM TARİHİ</label>
                                        <input class="form-control" type="date" id="DOGUM_TARIHI" name="DOGUM_TARIHI" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="firstName" class="form-label">DOĞUM YERİ İL</label>
                                        <input class="form-control" type="text" id="DOGUM_YERI_IL" name="DOGUM_YERI_IL" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="firstName" class="form-label">DOĞUM YERİ İLÇE</label>
                                        <input class="form-control" type="text" id="DOGUM_YERI_ILCE" name="DOGUM_YERI_ILCE" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="firstName" class="form-label">ÖĞRENİM DURUMU</label>
                                        <input class="form-control" type="text" id="OGRENIM_DURUMU" name="OGRENIM_DURUMU" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col-4">
                                        <label for="firstName" class="form-label">MEZUN OLDUĞU OKUL</label>
                                        <input class="form-control" type="text" id="MEZUN_OKUL" name="MEZUN_OKUL" disabled>
                                    </div>


                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="firstName" class="form-label">MEZUN OLDUĞU BOLUM</label>
                                        <input class="form-control" type="text" id="MEZUN_BOLUM" name="MEZUN_BOLUM" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="firstName" class="form-label">MEZUN OLDUĞU YIL</label>
                                        <input class="form-control" type="text" id="MEZUN_YIL" name="MEZUN_YIL" oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');" maxlength="4" disabled>
                                    </div>
                                </div>

                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="firstName" class="form-label">İKAMETGAH İLİ</label>
                                        <input class="form-control" type="text" id="IKAMETGAH_IL" name="IKAMETGAH_IL" disabled>
                                    </div>

                                </div>
                                <div class="row mt-3">

                                    <div class="col">
                                        <label for="firstName" class="form-label">İKAMETGAH İLÇESİ</label>
                                        <input class="form-control" type="text" id="IKAMETGAH_ILCE" name="IKAMETGAH_ILCE" disabled>
                                    </div>

                                </div>
                                <div class="row mt-3">

                                    <div class="col">
                                        <label for="firstName" class="form-label">İKAMETGAH ADRESİ</label>
                                        <textarea row="3" class="form-control" type="text" id="IKAMETGAH_ADRES" name="IKAMETGAH_ADRES" disabled></textarea>
                                    </div>
                                </div>

                                <div class="row mt-3">
                                    <div class="col-4">
                                        <label for="MEDENI_HAL" class="form-label">MEDENİ HALİ</label>
                                        <select id="MEDENI_HAL" name="MEDENI_HAL" class="form-select" disabled>
                                            <option></option>
                                            <option value="Bekar">Bekar</option>
                                            <option value="Evli">Evli</option>

                                        </select>
                                    </div>
                                    <div class="col-4">
                                        <label for="ES_CALISMA_DURUMU" class="form-label">EŞ ÇALIŞMA DURUMU</label>
                                        <select id="ES_CALISMA_DURUMU" name="ES_CALISMA_DURUMU" class="form-select" disabled>
                                            <option></option>
                                            <option value="Evet">Evet</option>
                                            <option value="Hayır">Hayır</option>
                                            <option value="Emekli">Emekli</option>

                                        </select>
                                    </div>
                                    <div class="col-4">
                                        <label for="COCUK_SAYI" class="form-label">ÇOCUK SAYISI</label>
                                        <select id="COCUK_SAYI" name="COCUK_SAYI" class="form-select" disabled>
                                            <option></option>
                                            <option value="0">0</option>
                                            <option value="1">1</option>
                                            <option value="2">2</option>
                                            <option value="3">3</option>
                                            <option value="4">4</option>
                                            <option value="5">5</option>
                                            <option value="6">6</option>
                                            <option value="7">7</option>
                                            <option value="8">8</option>
                                            <option value="9">9</option>
                                            <option value="10">10</option>

                                        </select>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col-6">
                                        <label class="form-label">EŞİN ÇALIŞTIĞI FİRMA</label>
                                        <input class="form-control" type="text" id="ES_CALISMA_FIRMA" name="ES_CALISMA_FIRMA" disabled>
                                    </div>
                                    <div class="col-6">
                                        <label class="form-label">EŞİN ÜNVANI</label>
                                        <input class="form-control" type="text" id="ES_UNVANI" name="ES_UNVANI" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="IKAMET_EV_DURUM" class="form-label">İKAMET ETTİĞİNİZ EVİN DURUMU</label>
                                        <select id="IKAMET_EV_DURUM" name="IKAMET_EV_DURUM" class="form-select" disabled>
                                            <option></option>
                                            <option value="AİLE EVİ (SAHİP)">AİLE EVİ (SAHİP)</option>
                                            <option value="AİLE EVİ (KİRA)">AİLE EVİ (KİRA)</option>
                                            <option value="KENDİ EVİ (SAHİP)">KENDİ EVİ (SAHİP)</option>
                                            <option value="KENDİ EVİ (KİRA)">KENDİ EVİ (KİRA)</option>
                                            <option value="DİĞER">DİĞER</option>

                                        </select>
                                    </div>
                                    <div class="col">
                                        <label for="ARAC_SAHIP" class="form-label">KENDİNİZE AİT ARACINIZ VAR MI?</label>
                                        <select id="ARAC_SAHIP" name="ARAC_SAHIP" class="form-select" disabled>
                                            <option></option>
                                            <option value="Evet">Evet</option>
                                            <option value="Hayır">Hayır</option>


                                        </select>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="EHLIYET_VAR" class="form-label">EHLİYETİNİZ VAR MI?</label>
                                        <select id="EHLIYET_VAR" name="EHLIYET_VAR" class="form-select" disabled>
                                            <option></option>
                                            <option value="Evet">Evet</option>
                                            <option value="Hayır">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="col">
                                        <label for="EHLIYET_SINIF" class="form-label">EHLİYETİNİZ SINIFI</label>
                                        <select id="EHLIYET_SINIF" name="EHLIYET_SINIF" class="form-select" disabled>
                                            <option></option>
                                            <option value="M">M</option>
                                            <option value="A1">A1</option>
                                            <option value="A2">A2</option>
                                            <option value="A">A</option>
                                            <option value="B1">B1</option>
                                            <option value="B">B</option>
                                            <option value="BE">BE</option>
                                            <option value="C1">C1</option>
                                            <option value="C1E">C1E</option>
                                            <option value="C">C</option>
                                            <option value="CE">CE</option>
                                            <option value="D1">D1</option>
                                            <option value="D1E">D1E</option>
                                            <option value="D">D</option>
                                            <option value="DE">DE</option>
                                            <option value="F">F</option>
                                            <option value="G">G</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label class="form-label">EFECE GALVANİZ A.Ş.'DE İŞE BAŞLADIĞINIZ TARİH</label>
                                        <input class="form-control" type="date" id="MEVCUT_IS_ILK_TARIH" name="MEVCUT_IS_ILK_TARIH" disabled>
                                    </div>
                                    <div class="col">
                                        <label class="form-label">EFECE GALVANİZ A.Ş.'DEN AYRILIŞ TARİHİ</label>
                                        <input class="form-control" type="date" id="IS_CIKIS_TARIH" name="IS_CIKIS_TARIH" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label class="form-label">ÇALIŞTIĞINIZ BİRİM</label>
                                        <input class="form-control" type="text" id="CALISILAN_BIRIM" name="CALISILAN_BIRIM" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label class="form-label">ÇALIŞTIĞINIZ GÖREV</label>
                                        <input class="form-control" type="text" id="GOREV" name="GOREV" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label class="form-label">ÇALIŞMA HAYATINA BAŞLADIĞINIZ TARİH</label>
                                        <input class="form-control" type="date" id="ILK_IS_TARIH" name="ILK_IS_TARIH" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col-4">
                                        <label for="KAN_GRUP" class="form-label">KAN GRUBUNUZ</label>
                                        <select id="KAN_GRUP" name="KAN_GRUP" class="form-select" disabled>
                                            <option></option>
                                            <option value="0 RH(+)">0 RH(+)</option>
                                            <option value="0 RH(-)">0 RH(-)</option>
                                            <option value="A RH(+)">A RH(+)</option>
                                            <option value="A RH(-)">A RH(-)</option>
                                            <option value="B RH(+)">B RH(+)</option>
                                            <option value="B RH(-)">B RH(-)</option>
                                            <option value="AB RH(+)">AB RH(+)</option>
                                            <option value="AB RH(-)">AB RH(-)</option>
                                        </select>
                                    </div>
                                    <div class="col">
                                        <label class="form-label">VARSA SÜREKLİ HASTALIĞINIZ</label>
                                        <input class="form-control" type="text" id="VARSA_SUREKLI_HAST" name="VARSA_SUREKLI_HAST" disabled>
                                    </div>

                                </div>
                                <div class="row mt-3">


                                    <div class="col">
                                        <label class="form-label">VARSA ENGEL DURUMUNUZ</label>
                                        <input class="form-control" type="text" id="VARSA_ENGEL_DURUM" name="VARSA_ENGEL_DURUM" disabled>
                                    </div>
                                    <div class="col">
                                        <label class="form-label">VARSA SÜREKLİ KULLANDIĞINIZ İLAÇ</label>
                                        <input class="form-control" type="text" id="VARSA_SUREKLI_KULL_ILAC" name="VARSA_SUREKLI_KULL_ILAC" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label for="COVID_ASI_DURUM" class="form-label">COVİD-19 AŞISI OLDUNUZ MU?</label>
                                        <select id="COVID_ASI_DURUM" name="COVID_ASI_DURUM" class="form-select" disabled>
                                            <option></option>
                                            <option value="Evet">Evet</option>
                                            <option value="Hayır">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="col">
                                        <label for="KAC_DOZ_ASI" class="form-label">KAÇ DOZ COVİD-19 AŞISI OLDUNUZ?</label>
                                        <select id="KAC_DOZ_ASI" name="KAC_DOZ_ASI" class="form-select" disabled>
                                            <option></option>
                                            <option value="1">1</option>
                                            <option value="2">2</option>
                                            <option value="3">3</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col-6">
                                        <label class="form-label">İLETİŞİM BİLGİLERİNİZ (TEL) (5XX XXX XX XX)</label>
                                        <input class="form-control" type="tel" id="ILETISIM_BILGI_TEL" name="ILETISIM_BILGI_TEL" maxlength="19" disabled>
                                    </div>

                                </div>
                                <div class="row mt-3">

                                    <div class="col">
                                        <label class="form-label">İLETİŞİM BİLGİLERİNİZ (Efece uzantılı olmayan E-MAIL)</label>
                                        <input class="form-control" type="email" id="ILETISIM_BILGI_MAIL" name="ILETISIM_BILGI_MAIL" placeholder="Lütfen e-mail adresinizi girin" disabled>
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col">
                                        <label class="form-label">ACİL DURUMDA ULAŞILMASI GEREKEN KİŞİ</label>
                                        <input class="form-control" type="text" id="ACIL_DURUM_KISI" name="ACIL_DURUM_KISI" disabled>
                                    </div>

                                </div>
                                <div class="row mt-3">

                                    <div class="col">
                                        <label class="form-label">ACİL DURUMDA ULAŞILMASI GEREKEN KİŞİ İLETİŞİM BİLGİLERİNİZ (TEL) (5XX XXX XX XX)</label>
                                        <input class="form-control" type="tel" id="ACIL_DURUM_KISI_ILETISIM" name="ACIL_DURUM_KISI_ILETISIM" maxlength="19" disabled>
                                    </div>
                                </div>

                                <hr class="my-0 mt-3">
                                <div class="row mt-3">
                                    <div class="col">
                                        <button type="button" class="ekle btn btn-info float-end" onclick="myKaydetFunction()" disabled>GÜNCELLE</button>
                                    </div>


                                </div>


                            }
                        }
                        else
                        {
                            using (Html.BeginForm("PersonelSubeYonetim", "IK", FormMethod.Post, new { @id = "myPersonalForm" }))
                            {
                                if (Session["Alert"] != null)
                                {
                                    if (Session["Alert"].ToString() == "Personel başarılı bir şekilde eklenmiştir.")
                                    {
                                        <div class="alert alert-success" role="alert">
                                            <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
                                            @Session["Alert"].ToString()
                                        </div>
                                    }
                                    else if (Session["Alert"].ToString() == "Personel kaydı başarısız!")
                                    {
                                        <div class="alert alert-danger" role="alert">
                                            <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
                                            @Session["Alert"].ToString()
                                        </div>
                                    }
                                }
                                if (ViewBag.Yetki != null && ViewBag.Ekle == null)
                                {
                                    <div class="row mt-1">

                                        <div class="col-2">
                                            <label for="firstName" class="form-label">Personel Kimlik Numarası</label>
                                            <input class="form-control duzenle" type="text" value=@ViewBag.Personel.USER_ID id="USER_ID" name="USER_ID" readonly>
                                        </div>


                                    </div>
                                    <div class="row mt-2">
                                        <div class="col">

                                            <label for="firstName" class="form-label">İsim</label>
                                            <input class="form-control duzenle" type="text" value=@ViewBag.Personel.ISIM id="ISIM" name="ISIM">

                                        </div>
                                        <div class="col">

                                            <label for="firstName" class="form-label">Soyisim</label>
                                            <input class="form-control duzenle" type="text" id="SOYISIM" name="SOYISIM" value=@ViewBag.Personel.SOYISIM>


                                        </div>
                                    </div>

                                    <div class="row mt-2">
                                        <div class="col-6">
                                            <label for="firstName" class="form-label">TC KİMLİK NUMARASI</label>
                                            @if (ViewBag.Personel.TCKN != null)
                                            {
                                                <input class="form-control duzenle" type="text" id="TCKN" name="TCKN" value=@ViewBag.Personel.TCKN oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');" maxlength="11">

                                            }
                                            else
                                            {
                                                <input class="form-control duzenle" type="text" id="TCKN" name="TCKN" value="" oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');" maxlength="11">

                                            }

                                        </div>
                                        <div class="col-6">
                                            <label for="CINSIYET" class="form-label">CİNSİYET</label>
                                            <select id="CINSIYET" name="CINSIYET" class="form-select duzenle">

                                                @if (ViewBag.Personel.CINSIYET == "KADIN")
                                                {


                                                    <option value="KADIN" selected>KADIN</option>


                                                    <option value="ERKEK">ERKEK</option>
                                                }
                                                else if (ViewBag.Personel.CINSIYET == "ERKEK")
                                                {




                                                    <option value="KADIN">KADIN</option>






                                                    <option value="ERKEK" selected>ERKEK</option>
                                                }
                                                else
                                                {
